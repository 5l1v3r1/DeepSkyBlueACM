
<html lang="en">

<head>
    <title>Bluppy Bird</title>
    <link rel="shortcut icon" type="image/png" href="img/faveicon.png">
    <meta name="description" content="بازی کنید و پیکسل برنده بشید!" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
    <!-- Style sheets -->
    <link href="game/css/reset.css" rel="stylesheet">
    <link href="game/css/main.css" rel="stylesheet">
    <script src="dist/sweetalert2.min.js"></script>
    <link rel="stylesheet" href="dist/sweetalert2.min.css">

    <script src="game/js/jquery.min.js"></script>
    <script src="game/js/jquery.transit.min.js"></script>
    <script src="game/js/buzz.min.js"></script>


</head>
<body>
<div id="gamecontainer">
    <div id="gamescreen">
        <div id="sky" class="animated">
            <div id="flyarea">
                <div id="plane" class="animated"></div>
                <div id="ceiling" class="animated"></div>
                <!-- This is the flying and pipe area container -->
                <div id="player" class="bird animated"></div>
                <div id="bigscore"></div>
                <div id="splash"></div>
                <div id="scoreboard">
                    <div id="medal"></div>
                    <div id="currentscore"></div>
                    <div id="highscore"></div>
                    <div id="replay"><img src="game/assets/replay.png" alt="replay"></div>
                </div>
                <!-- Pipes go here! -->
            </div>
        </div>

        <div id="land" class="animated"><div id="debug"></div></div>
    </div>
</div>
<div class="boundingbox" id="playerbox"></div>
<div class="boundingbox" id="pipebox"></div>

<script src="game/js/main.js"></script>
</body>
</html>